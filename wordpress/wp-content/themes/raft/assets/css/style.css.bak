.page-header:before {
  content: "";
  width: 100%;
  height: 100%;
  background: url(../images/bg-noise.gif);
  opacity: 0.04;
  position: absolute;
  left: 0;
  top: 0;
}
.page-header:after {
  content: "";
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: url(../images/bg-lines-2.png) center;
  z-index: 1;
}
.page-header .container {
  position: relative;
  z-index: 1;
  color: #fff;
}
.page-header .container h2 {
  width: 100%;
  display: block;
  font-family: "Barlow","Sawarabi Gothic","Quicksand","Source Sans Pro","-apple-system","BlinkMacSystemFont","Segoe UI","Roboto","Helvetica Neue","Arial",sans-serif;
  font-size: 80px;
  font-weight: 700;
  margin-bottom: 0;
}
.page-header .container h2:before {
  content: attr(data-text);
